    "CreatePR": {
      "additionalProperties": false,
      "description": "Creates a pull request using the `gh` CLI.",
      "properties": {
        "comment": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Comment"
        },
        "directory_statuses": {
          "additionalProperties": {
            "enum": [
              "not-started",
              "failed",
              "completed"
            ],
            "type": "string"
          },
          "default": {},
          "propertyNames": {
            "format": "directory-path"
          },
          "title": "Directory Statuses",
          "type": "object"
        },
        "slug": {
          "const": "create-pr",
          "title": "Slug",
          "type": "string"
        },
        "config": {
          "$ref": "#/$defs/potent__operations__create_pr__Config"
        }
      },
      "required": [
        "slug",
        "config"
      ],
      "title": "CreatePR",
      "type": "object"
    },
